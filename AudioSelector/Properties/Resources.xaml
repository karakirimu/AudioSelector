<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Setting window strings -->
    <sys:String x:Key="SettingTitle">Settings</sys:String>
    <sys:String x:Key="SettingGeneral">General</sys:String>
    
    <!-- General UserControl strings -->
    <sys:String x:Key="GeneralTheme">Theme</sys:String>
    <sys:String x:Key="ThemeSystem">System</sys:String>
    <sys:String x:Key="ThemeLight">Light</sys:String>
    <sys:String x:Key="ThemeDark">Dark</sys:String>
    <sys:String x:Key="GeneralLanguage">Language</sys:String>
    <sys:String x:Key="LanguageSystem">System</sys:String>
    <sys:String x:Key="GeneralHotkey">Hotkey</sys:String>
    <sys:String x:Key="GeneralStartup">Launch at startup</sys:String>

</ResourceDictionary>